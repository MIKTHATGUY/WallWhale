{
  "name": "@wallwhale/wallwhale",
  "version": "1.0.0",
  "description": "Enterprise-grade WallWhale management API with authentication, monitoring, and scalability features",
  "private": true,
  "type": "module",
  "keywords": [
    "steam",
    "wallwhale",
    "api",
    "fastify",
    "typescript",
    "enterprise"
  ],
  "author": {
    "name": "WallWhale",
    "email": "engineering@wallwhale.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wallwhale/wallwhale.git"
  },
  "bugs": {
    "url": "https://github.com/wallwhale/wallwhale/issues"
  },
  "homepage": "https://github.com/wallwhale/wallwhale#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:debug": "tsx watch --inspect src/index.ts",
    "build": "npm run clean && tsc -p tsconfig.json",
    "build:prod": "npm run clean && tsc -p tsconfig.prod.json",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "vitest run test/e2e",
    "test:integration": "vitest run test/integration",
    "test:unit": "vitest run test/unit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "accounts:encode": "node scripts/accounts.js encode",
    "accounts:decode": "node scripts/accounts.js decode",
    "accounts:help": "node scripts/accounts.js help",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "cert:setup": "tsx src/index.ts cert setup",
    "cert:generate": "tsx src/index.ts cert generate",
    "cert:mkcert": "tsx src/index.ts cert mkcert",
    "cert:letsencrypt": "tsx src/index.ts cert letsencrypt",
    "cert:check": "tsx src/index.ts cert check",
    "setup": "tsx src/index.ts setup",
    "setup:quick": "tsx src/index.ts quick-setup",
    "setup:build-downloader": "tsx src/index.ts build-depot",
    "check": "tsx src/index.ts check",
    "docker:build": "docker build -t wallwhale .",
    "docker:run": "docker run -p 3000:3000 --name wallwhale-server wallwhale",
    "docker:stop": "docker stop wallwhale-server",
    "docker:remove": "docker rm wallwhale-server",
    "docker:logs": "docker logs -f wallwhale-server",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:start": "bash scripts/docker/start.sh",
    "docker:start:win": "scripts\\docker\\start.bat",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "precommit": "lint-staged",
    "prepare": "husky install || true",
    "docs:generate": "typedoc --out docs/api src",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test"
  },
  "dependencies": {
    "@fastify/cors": "11.1.0",
    "@fastify/helmet": "13.0.1",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/sensible": "6.0.3",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@fastify/under-pressure": "9.0.3",
    "@prisma/client": "6.14.0",
    "@sinclair/typebox": "0.34.38",
    "acme-client": "5.4.0",
    "archiver": "7.0.1",
    "bcryptjs": "^2.4.3",
    "boxen": "8.0.1",
    "chalk": "5.5.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cli-spinners": "3.2.0",
    "commander": "14.0.0",
    "compression": "1.8.1",
    "difflib": "0.2.4",
    "dotenv": "17.2.1",
    "fastify": "5.5.0",
    "fastify-graceful-shutdown": "^3.5.0",
    "fastify-metrics": "12.1.0",
    "fastify-plugin": "5.0.1",
    "figlet": "^1.8.2",
    "gradient-string": "3.0.0",
    "inquirer": "12.9.1",
    "ioredis": "5.7.0",
    "jsonwebtoken": "9.0.2",
    "nodemailer": "^6.9.14",
    "ora": "8.2.0",
    "pino": "9.8.0",
    "pino-http": "10.5.0",
    "reflect-metadata": "0.2.2",
    "selfsigned": "3.0.1",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/archiver": "6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "1.8.1",
    "@types/difflib": "0.2.7",
    "@types/figlet": "^1.7.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/node": "24.2.1",
    "@types/node-forge": "^1.3.13",
    "@types/nodemailer": "6.4.17",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "3.0.1",
    "globals": "^16.3.0",
    "husky": "9.1.7",
    "lint-staged": "^15.2.8",
    "node-forge": "1.3.1",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "rimraf": "6.0.1",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.20.4",
    "typedoc": "0.28.10",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
